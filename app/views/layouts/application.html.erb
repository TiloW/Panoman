<!DOCTYPE html>
<html>
<head>
  <title><%= get_title %></title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= yield :head %>
  <%= csrf_meta_tag %>
</head>
<body>
  <%= render 'layouts/header' %>
  <div id="content" class="round full-width">
    <%= yield %>
  </div>
  <%= render 'layouts/footer' %>
</body>
</html>