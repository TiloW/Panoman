    <div id="grid">
      <ul> <% @city.panoramas.keep_if{ |d| !d.district.blank? }.sort_by{ |p| p.priority }.each do |pano| %>        
        <li><% panoname = (pano.alt_name.blank? ? (pano.name) : (pano.alt_name)) %>
          <div class="wrapper">
            <a href="http://www.stadtpanoramen.de/<%= @city.get_filename %>/<%= pano.get_filename %>.html" title="<%= panoname %> <%= pano.location %> in <%= @city.name %> <%= pano.district %>"><img src="http://panorama-cities.net/<%= FileHelper.to_filename @city.en_name %>/panoramas/<%= pano.get_filename %>_<%= @city.get_filename %>.jpg" alt="<%= pano.name %> :::-<%= @city.name %>"></a>
            <% unless pano.rotation == 60 %>
            <div class="widescreen"><a href="javascript:cindex('<%= pano.get_filename %>_5c.html')" title="Panorama <%= pano.name %> in voller Breite">Widescreen</a>
            </div><% end %>
          </div><%= pano.name %>
        </li><% end %>
      </ul>
