      <div id="menu">  
        <h2>Panoramen <%= @city.name %> A-Z</h2>
        <nav>
          <ul><% @city.panoramas.keep_if{ |p| p.published }.sort_by{ |n| n.name }.each do |pano| %><% panoname = (pano.alt_name.blank? ? (pano.name) : (pano.alt_name)) %>
            <li><a href="<%= pano.get_filename %>.html" title="<%= panoname %> <%= pano.location %> <%= pano.district %>"><%= pano.name %> </a></li><% end %>
          </ul>
        </nav>
      </div>  
     <div id="content-top">     
        <div id="box">  <% if !@city.ad_more_first_page_top.blank? %>
      <div class="adv-more">
        <%= @city.ad_more_first_page_top %>
      </div> 
      <% end %> <% if !@city.linktips.blank? %>
        <div id="tip">
          <%= @city.linktips %>
        </div>
         <div class="g_adv_linktips">
              <%= @city.ad_mobile_banner %>
         </div><% else %>
         <div class="g_adv_only">
              <%= @city.ad_mobile_banner %>
         </div><% end %>
      </div>                
      <div id="grid">
        <ul> <% @city.panoramas.keep_if{ |p| p.published }.sort_by{ |p| p.priority }.first(11).each do |pano| %>      
          <li><% panoname = (pano.alt_name.blank? ? (pano.name) : (pano.alt_name)) %>
            <div class="wrapper">
              <a href="http://www.stadtpanoramen.de/<%= @city.get_filename %>/<%= pano.get_filename %>.html" title="<%= panoname %> <% unless pano.location.nil? %><%= pano.location + " " %><% end %>in <%= @city.name %> <%= pano.district %>"><img src="http://panorama-cities.net/<%= FileHelper.to_filename @city.en_name %>/panoramas/<%= pano.get_filename %>_<%= @city.get_filename %>.jpg" width="350" height="144" alt="<%= pano.name %> <%= pano.location %> <%= @city.name %><%= pano.district %>"></a>    <% unless pano.rotation == 60 %>
              <div class="widescreen"><a href="javascript:cindex('<%= pano.get_filename %>_5c.html')" title="Panorama <%= pano.name %> in voller Breite">Widescreen</a>
              </div><% end %>
            </div><%= pano.name %>
          </li><% end %>
              <% line1="" %>  
              <% panpub = @city.panoramas.keep_if{ |p| p.published }.count %>
              <% if panpub > 15 %>
              <% line1 = '<span style="font-size: 1.1em;"><a href="' + @city.get_filename + '_panoramen.html#' + @city.get_filename + '">Mehr Panoramen von ' + @city.name + '</a></span> <br><br>' %>
             <% end %>            
              
              <% line2="" %>  
              <% districts = Hash.new
              @city.panoramas.each { |p| districts[p.district] = true if p.district }
              districts.each_key do |dist| %>
              <% unless dist.blank? %>
              <% distfile =  FileHelper.to_filename dist %>
              <% line2 += '<a href="panoramen_' + distfile + '.html"> Panoramen ' + dist + ' </a><br><br>' %>            
             <% end %>
              <% end %>
              
              <% line3="" %>  
              <% locations = Hash.new
              @city.panoramas.each { |p| locations[p.location] = true if p.location }
              locations.each_key do |loc| %>
              <% unless loc.blank? %>
              <% locfile =  FileHelper.to_filename loc %><% if  ( loc == "Ferienwohnung Tecklenburger Augenblicke" || loc == "Hotel Bismarckhöhe" || loc == "Landhotel Martinshof" || loc == "Hotel Königswache") %><% folder = locfile %>
              <% line3 += '<a href="' + folder + '/index.html">Panoramen ' + loc + ' </a><br><br>' %><% else %>
              <% line3 += '<a href="panoramen_' + locfile + '.html">Panoramen ' + loc + ' </a><br><br>' %><% end %>
              <% end %>
              <% end %>
              
              <% line4="" %>  
              <% if @city.quiz == true %>
              <% line4 = '<a href="http://www.stadtpanoramen.de/' + @city.get_filename + '/' + @city.get_filename + '_quiz.html">' + @city.name + '-Quiz  </a>' %>
              <% end %> 
              
              <% @city.name == 'Münster' ? panolink =  '<a href="../muensterland/index.html">Panoramen Münsterland </a><br><br>'  : (@city.name == 'Köln' ? panolink =  '<a href="http://www.stadtpanoramen.de/phantasialand/">Panoramen Phantasialand </a><br><br>'  : (@city.name == 'Osnabrück' ? panolink =  '<a href="http://www.stadtpanoramen.de/bramsche/">Panoramen Bramsche </a><br><br>'  : panolink='')) %>
              <% if !line1.blank? or !line2.blank? or !line3.blank? or !line4.blank? or !panolink.blank? %>            
             <%= '
          <li style="margin-bottom: 40px; width:348px; border: 1px solid #444;">
            <div class="tabl">  
              <div class="list_more">' +  line1 + line2 + line3 + panolink + line4 +'
            </div>
          </div>
         </li>' %>
              <% end %>
        </ul>
