          end.sort_by{ |p| p.priority }.each do |pano| %>   
        <li><% pano_subtitle = (pano.en_name.blank? ? (pano.alt_en_name.blank? ? (pano.name) : (pano.alt_en_name)) : (pano.en_name)) %>  <% pano_enname = (pano.alt_en_name.blank? ? (pano.en_name.blank? ? (pano.name) : (pano.en_name)) : (pano.alt_en_name)) %>
          <div class="wrapper"><% pano_enlocation = (pano.en_location.blank? ? (pano.location.blank? ? (" ") : (pano.location)) : (pano.en_location)) %>
            <a href="http://www.panorama-cities.net/<%= FileHelper.to_filename @city.en_name %>/<%= pano.en_name.blank? ? (FileHelper.to_filename pano.name) : (FileHelper.to_filename pano.en_name) %>.html" title=" <%= pano_enname %> <%= pano_enlocation %> in <%= @city.en_name %> <%= pano.district %>"><img src="http://panorama-cities.net/<%= FileHelper.to_filename @city.en_name %>/panoramas/<%= FileHelper.to_filename pano.name %>_<%= FileHelper.to_filename @city.name %>.jpg" alt="<%= pano.name + " " + @city.name %>"></a>
            <% unless pano.rotation == 60 %>  
            <div class="widescreen"><a href="javascript:cindex('http://www.panorama-cities.net/<%= FileHelper.to_filename @city.en_name %>/<%= pano.en_name.blank? ? (FileHelper.to_filename pano.name) : (FileHelper.to_filename pano.en_name) %>_5c.html')" title="<%= pano_enname %> Panorama in Widescreen">Widescreen</a>
            </div><% end %>
          </div><%= pano_subtitle %>
        </li><% end %>
